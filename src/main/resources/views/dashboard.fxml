<VBox fx:id="vbRootHome" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onDragDropped="#dragDropped" onDragEntered="#dragEntered" onDragExited="#dragExited" onDragOver="#dragOver" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="acat.controller.Dashboard">
   <children>
      <ImageView fitHeight="189.0" fitWidth="288.0" pickOnBounds="true" preserveRatio="true">
