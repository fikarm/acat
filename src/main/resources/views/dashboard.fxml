            <Insets bottom="20.0" />
         </VBox.margin>
      </ImageView>
      <Label focusTraversable="false" text="Drag and drop java project folder here">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></Label>
      <Button fx:id="btnBrowse" mnemonicParsing="false" onAction="#openFolder" text="or browse from your computer">
         <padding>
            <Insets bottom="8.0" left="18.0" right="18.0" top="8.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-blue" />
